  "bugs": {
    "url": "https://github.com/Irina-N23/Integration_With_AT_Structure_and_3d-Party_Tools/issues"
  },
  "homepage": "https://github.com/Irina-N23/Integration_With_AT_Structure_and_3d-Party_Tools#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.2.0",
    "cucumber-junit-convert": "^1.1.0",
    "cucumber-pretty": "^6.0.0",
    "fs-extra": "^9.0.1",
    "protractor": "^7.0.0",
    "protractor-cucumber-framework": "^6.2.1",
