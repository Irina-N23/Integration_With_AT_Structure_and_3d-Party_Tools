{
  "name": "integration_with_at_structure_and_3d-party_tools",
  "version": "1.0.0",
  "description": "a framework with test reporting with javascript libraries",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "protractor ./test/config/protractor-config.js",
    "remove-logs": "node ./test/utilities/cleanUp/logs-cleaner.js",
    "remove-reports": "node ./test/utilities/cleanUp/reports-cleaner.js",
    "pretest": "npm run remove-logs && npm run remove-reports",
    "update": "webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Irina-N23/Integration_With_AT_Structure_and_3d-Party_Tools.git"
  },
  "keywords": [
    "test-reporting",
    "cucumber-pretty",
    "screenshot",
    "framework",
    "javascript"
  ],
